<div class="card">
  <div class="card-header">
   <h4> Sign In </h4>
  </div>
  <div class="card-body">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="field form-group">
      <%= f.label :email %><br />
      <%= f.email_field :email, class: "form-control", autofocus: true, autocomplete: "Example: student.1@osu.edu" %>
    </div>
  
    <div class="field form-group">
      <%= f.label :password %><br />
      <%= f.password_field :password, class: "form-control", autocomplete: "current-password" %>
    </div>
  
    <% if devise_mapping.rememberable? %>
      <div class="field">
        <%= f.check_box :remember_me %>
        <%= f.label "Stay signed in?"%>
      </div>
    <% end %>
  
    <div class="actions">
      <%= f.submit "Log in", class: 'btn btn-outline-success' %>
      <%= render "devise/shared/links" %>
    </div>
  <% end %>
  </div>
</div>
</br>

